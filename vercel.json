{
   "builds": [
     {
       "src": "index.js",
       "use": "@vercel/node"
     },
     {
       "src": "build/**",
       "use": "@vercel/static"
     }
   ],



   "routes":[
    {
      "src":"/user",
      "dest":"index.js"
    },
    {
      "src":"/user/(.*)",
      "dest":"index.js"
    },
    {
      "src":"/product",
      "dest":"index.js"
    },
    {
      "src":"/product/(.*)",
      "dest":"index.js"
    }
    ,
    {
      "src":"/cart",
      "dest":"index.js"
    },
    {
      "src":"/cart/(.*)",
      "dest":"index.js"
    },
    {
      "src":"/address",
      "dest":"index.js"
    },
    {
      "src":"/address/(.*)",
      "dest":"index.js"
    },
    {
      "src":"/order",
      "dest":"index.js"
    },
    {
      "src":"/order/(.*)",
      "dest":"index.js"
    }
   ]
 
   
 }
